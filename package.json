{
  "name": "restaurant_search",
  "version": "1.0.0",
  "description": "A simple Spring Boot and React application to simulate a restaurant search service implementing custom composite sorting algorithms to rank local dining options based on user constraints.",
  "main": "index.js",
  "scripts": {
    "start": "npm run build:server && npm run dev",
    "build:server": "cd server && mvnw.cmd clean package",
    "dev": "concurrently \"npm run start:server\" \"npm run client\"",
    "start:server": "cd server && java -jar target/server-0.0.1-SNAPSHOT.jar",
    "client": "cd client && npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/purabtpatel/Restaurant_Search.git"
  },
  "keywords": [],
